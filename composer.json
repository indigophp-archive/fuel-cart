{
    "name": "indigophp/fuel-cart",
    "type": "fuel-package",
    "description": "Fuel wrapper around Indigo Cart package",
    "license": "MIT",
    "keywords": ["cart"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "indigophp/cart": "*"
    },
    "require-dev": {
        "indigophp/cart": "dev-develop",
        "codeception/codeception": "~2.0.0",
        "codeception/mockery-module": "dev-master",
        "indigophp/codeception-fuel-module": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Fuel\\": "classes/"
        }
    },
    "extra": {
        "installer-name": "cart"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
